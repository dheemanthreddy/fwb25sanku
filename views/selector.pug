extends layout

block content
  h1= title
  - let image_names = ["1.png","2.png","3.png","4.png","5.png"] 
  - for(let i=0;i<3;i++){
      - let randomNumber = Math.floor(Math.random()*5)
      - let imageNameVariable = image_names[randomNumber]

      img(src=`images/${imageNameVariable}` alt='icecream' style='width:300px; height:300px;')
      |    
  - }
        